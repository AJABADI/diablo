	keywords = {anti–interleukin-13 therapy, mild asthma, allergen inhalation challenge, late asthmatic response, airway inflammation},
	file = {Full Text PDF:/Users/asingh/Zotero/storage/RZANXFKX/Gauvreau et al. - 2011 - Effects of Interleukin-13 Blockade on Allergen-ind.pdf:application/pdf;Snapshot:/Users/asingh/Zotero/storage/JSEN2TR6/rccm.html:text/html}
