	langid = {english},
	pmid = {16644721},
	file = {Full Text PDF:/Users/asingh/Zotero/storage/U6GS2CCS/Yu et al. - 2006 - Regulation of Bruton Tyrosine Kinase by the Peptid.pdf:application/pdf;Snapshot:/Users/asingh/Zotero/storage/4AES7RE6/18201.html:text/html}
}

@article{liu_characterization_2013,
	title = {Characterization of Torin2, an {ATP}-Competitive Inhibitor of {mTOR}, {ATM}, and {ATR}},
