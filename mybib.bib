@article{buse_exenatide_2013,
	title = {Exenatide once weekly versus liraglutide once daily in patients with type 2 diabetes ({DURATION}-6): a randomised, open-label study},
	volume = {381},
	issn = {0140-6736, 1474-547X},
	url = {http://www.thelancet.com/journals/lancet/article/PIIS0140-6736(12)61267-7/abstract},
	doi = {10.1016/S0140-6736(12)61267-7},
	shorttitle = {Exenatide once weekly versus liraglutide once daily in patients with type 2 diabetes ({DURATION}-6)},
	abstract = {{\textless}h2{\textgreater}Summary{\textless}/h2{\textgreater}{\textless}h3{\textgreater}Background{\textless}/h3{\textgreater}{\textless}p{\textgreater}Glucagon-like peptide-1 receptor agonists exenatide and liraglutide have been shown to improve glycaemic control and reduce bodyweight in patients with type 2 diabetes. We compared the efficacy and safety of exenatide once weekly with liraglutide once daily in patients with type 2 diabetes.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}We did a 26 week, open-label, randomised, parallel-group study at 105 sites in 19 countries between Jan 11, 2010, and Jan 17, 2011. Patients aged 18 years or older with type 2 diabetes treated with lifestyle modification and oral antihyperglycaemic drugs were randomly assigned (1:1), via a computer-generated randomisation sequence with a voice response system, to receive injections of once-daily liraglutide (1·8 mg) or once-weekly exenatide (2 mg). Participants and investigators were not masked to treatment assignment. The primary endpoint was change in glycated haemoglobin ({HbA}$_{\textrm{1c}}$) from baseline to week 26. Analysis was by intention to treat. This trial is registered with {ClinicalTrials}.gov, number {NCT}01029886.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Findings{\textless}/h3{\textgreater}{\textless}p{\textgreater}Of 912 randomised patients, 911 were included in the intention-to-treat analysis (450 liraglutide, 461 exenatide). The least-squares mean change in {HbA}$_{\textrm{1c}}$ was greater in patients in the liraglutide group (−1·48\%, {SE} 0·05; n=386) than in those in the exenatide group (–1·28\%, 0·05; 390) with the treatment difference (0·21\%, 95\% {CI} 0·08–0·33) not meeting predefined non-inferiority criteria (upper limit of {CI} {\textless}0·25\%). The most common adverse events were nausea (93 [21\%] in the liraglutide group \textit{vs} 43 [9\%] in the exenatide group), diarrhoea (59 [13\%] {\textless}i{\textgreater}vs{\textless}/i{\textgreater} 28 [6\%]), and vomiting 48 [11\%] {\textless}i{\textgreater}vs{\textless}/i{\textgreater} 17 [4\%]), which occurred less frequently in the exenatide group and with decreasing incidence over time in both groups. 24 (5\%) patients allocated to liraglutide and 12 (3\%) allocated to exenatide discontinued participation because of adverse events.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Interpretation{\textless}/h3{\textgreater}{\textless}p{\textgreater}Both once daily liraglutide and once weekly exenatide led to improvements in glycaemic control, with greater reductions noted with liraglutide. These findings, plus differences in injection frequency and tolerability, could inform therapeutic decisions for treatment of patients with type 2 diabetes.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Funding{\textless}/h3{\textgreater}{\textless}p{\textgreater}Eli Lilly and Company and Amylin Pharmaceuticals {LLC}.{\textless}/p{\textgreater}},
	pages = {117--124},
	number = {9861},
	journaltitle = {The Lancet},
	shortjournal = {The Lancet},
	author = {Buse, John B. and Nauck, Michael and Forst, Thomas and Sheu, Wayne H.-H. and Shenouda, Sylvia K. and Heilmann, Cory R. and Hoogwerf, Byron J. and Gao, Aijun and Boardman, Marilyn K. and Fineman, Mark and Porter, Lisa and Schernthaner, Guntram},
	urldate = {2017-09-25},
	date = {2013-01-12},
	pmid = {23141817, 23141817},
	file = {Snapshot:/Users/asingh/Zotero/storage/BEQZUUHR/PIIS0140-6736(12)61267-7.html:text/html}
}

