	author = {Fu, Michael and Zhou, Jingmin and Thunström, Erik and Almgren, Torbjörn and Grote, Ludger and Bollano, Entela and Schaufelberger, Maria and Johansson, Magnus C. and Petzold, Max and Swedberg, Karl and Andersson, Bert},
